import sys
from collections import deque
a = sys.stdin.readline().strip()
b = sys.stdin.readline().strip()
cur = deque()
for i in range(8):
    cur.append(int(a[i]))
    cur.append(int(b[i]))
#print(cur)
answer = 0
while True:
    cur_max = len(cur)
    for i in range(cur_max):
        a = cur.popleft()
        b = cur.popleft()
        cur.appendleft(b)
        cur.append((a+b)%10)
    cur.pop()
    #print(cur)
    if len(cur) == 2:
        break
    
print(cur[0],end="")
print(cur[1])
    
